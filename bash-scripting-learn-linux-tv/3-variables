#echo $newvariable 
#dollar sign is to avoid name collisions

#!bin/bash

myname="Jay"
myage="32"

echo "Hello, my name is $myname."
echo "I'm $myage years old."
echo 'Hello, my name is $myname.'
echo 'I am $myage years old.'
#see the difference between quotes
